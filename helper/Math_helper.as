	public class Math_helper
	{

		/**
		 *
		 * @param degree
		 * @return
		 */
		public static function deg2rad(degree):Number
		{
			return degree * (Math.PI / 180);
		}

		/**
		 *
		 */
		public function Math_helper()
		{
		}
	}
